<?xml version="1.0"?>
   <robot name="origins">
     <link name="base_link">
       <visual>
         <geometry>
           <cylinder length="0.6" radius="0.3"/>
         </geometry>
       </visual>
     </link>
   
     <link name="right_leg">
       <visual>
         <geometry>
           <box size="1 0.075 0.6"/>
         </geometry>
         <origin rpy="0 0 -0.7855" xyz="0.5 -0.5 0"/>
       </visual>
     </link>
   
     <joint name="base_to_right_leg" type="revolute">
       <parent link="base_link"/>
       <child link="right_leg"/>
