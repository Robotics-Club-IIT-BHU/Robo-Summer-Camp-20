     </joint>

     <link name="right_leg1">
       <visual>
         <geometry>
           <box size="1 0.08 0.6"/>
         </geometry>
         <origin rpy="0 0 -1.57" xyz="-0.075 -0.4 0"/>
       </visual>
     </link>

     <joint name="right_leg_to_right_leg1" type="revolute">
       <parent link="right_leg"/>
       <child link="right_leg1"/>
       <origin xyz="0.907 -0.907 0"/>
     </joint>

   </robot>
